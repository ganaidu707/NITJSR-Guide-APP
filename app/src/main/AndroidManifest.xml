<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myapps.nitjsr.guide">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/unnamed"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/unnamed"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.myapps.nitjsr.guide.welcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.MainActivity"
            android:parentActivityName="com.myapps.nitjsr.guide.welcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="com.exampleone.nitjsr.firstapp.MainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="firstapp" android:host="path" />
            </intent-filter>

        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.firstactivity"
            android:parentActivityName="com.myapps.nitjsr.guide.AnimationActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.AnimationActivity"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.People"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.Admissions"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.Social"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.fests"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.food"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.Contacts"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.Quicklinks"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.Departments"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.about"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.notifications"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.aboutapp"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.Developer"
            android:parentActivityName="com.myapps.nitjsr.guide.MainActivity">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.InsideCampus"
            android:parentActivityName="com.myapps.nitjsr.guide.food">
        </activity>

        <activity
            android:name="com.myapps.nitjsr.guide.hallFood"
            android:parentActivityName="com.myapps.nitjsr.guide.food">
        </activity>

        <activity android:name="com.myapps.nitjsr.guide.OutCampus"
            android:parentActivityName="com.myapps.nitjsr.guide.food">
        </activity>

    </application>

</manifest>